package ex07;

public class AccountTest {

	public static void main(String[] args) {
		
		Account account = new Account();
		
		account.setBalance(10000);

	}

}

class Account {
	private int balance;

	public int getBalance() {
		return balance;
	}

	public void setBalance(int balance) {
		this.balance = balance;
	}
	
	
}
