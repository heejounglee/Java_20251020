package ex06;

public class ClassEx01 {

	public static void main(String[] args) {
		Student s1 = new Student();
		s1.study();
		System.out.println("-------------------");
		Student s2 = new Student();
		s2.study();
	}

}

class Student {
	
	//멤버 변수는 무조건 private 접근 제어자를 사용 !!
	private String name;
	private int age;
	private String address;
	
	public void study() {
		System.out.println(name + " Java 공부 중 입니다.");
	}
}
