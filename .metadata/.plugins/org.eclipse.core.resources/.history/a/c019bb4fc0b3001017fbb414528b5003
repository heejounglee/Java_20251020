package ex01;

public class ClassEx01 {

	public static void main(String[] args) {
		Student s1 = new Student();
		s1.name = "";
		s1.age = 29;
		s1.adderss= "";
		s1.study();
		
		System.out.println("-------------------");
		Student s2 = new Student();
		s2.name = "";
		s2.age = 28;
		s2.adderss= "";
	}

}

class Student {
	String name;
	int age;
	String adderss;
	
	void study() {
		System.out.println(name + " Java 공부 중 입니다.");
	}
}
