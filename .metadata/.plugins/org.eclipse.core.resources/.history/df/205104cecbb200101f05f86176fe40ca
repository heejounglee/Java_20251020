package ex01;

/*
 * 1~10까지의 홀수 합 구하기
 * 단, continue 사용
 */
public class ContinueExample02 {

	public static void main(String[] args) {
		int sum=0;
		
		for(int i=1;i<=10;i++) {
			/*
			if (i%1 == 1)
				sum+=i;
			*/
			
			if(i%2 == 0)
				continue;
			sum += i;
		}
		System.out.println(sum);
	}

}
